{
    "name": "Node.js & TypeScript",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "VARIANT": "16-bullseye"
        }
    },
    "features": {
        "dotnet": "6.0"
    },
    "extensions": [
		"ms-dotnettools.vscode-dotnet-runtime",
		"microsoft-IsvExpTools.powerplatform-vscode",
		"GitHub.copilot",
		"GitHub.copilot-labs",
		"ms-dotnettools.dotnet-interactive-vscode"
	],
    "forwardPorts": [
        8181
    ],
    "remoteUser": "node"
}